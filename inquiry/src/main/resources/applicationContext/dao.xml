<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/context
		  http://www.springframework.org/schema/context/spring-context-2.5.xsd
          http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<!-- DAO Bean Starts -->
		<bean id="userDAO" class="com.royaltechnosoft.inquiry.dao.impl.UserDAOImpl" autowire="byName"/>
		<bean id="courseDAO" class="com.royaltechnosoft.inquiry.dao.impl.CourseDAOImpl" autowire="byName"/>
		<bean id="inquiryDAO" class="com.royaltechnosoft.inquiry.dao.impl.InquiryDAOImpl" autowire="byName"/>
		<bean id="followupDAO" class="com.royaltechnosoft.inquiry.dao.impl.FollowupDAOImpl" autowire="byName"/>
		<bean id="passwordRequestDAO" class="com.royaltechnosoft.inquiry.dao.impl.PasswordResetRequestDAOImpl" autowire="byName"/>
	<!-- DAO Bean Ends -->
	
</beans>